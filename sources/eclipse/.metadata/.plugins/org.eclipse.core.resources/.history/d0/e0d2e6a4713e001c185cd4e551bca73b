package model;

public class usuario {
	
	private String login;
	private String senha;
	private String permissao;
	
	//Método construtor simples
	public usuario() {
		super();
	}

	public usuario(String login, String senha, String permissao) {
		super();
		this.login = login;
		this.senha = senha;
		this.permissao = permissao;
	}
	
	
	
	
	
}
